background no
no_buffers yes
out_to_x no
out_to_console no
update_interval 1.0
short_units yes
overwrite_file /tmp/conkytext.tmp

TEXT
uptime:${uptime_short}  \
proc:${processes}    \
cpu:${cpu}%  \
mem:${memperc}%  \
swap:${swapperc}%    \
fs:${fs_used_perc /}%  \
io:${diskio}    \
addr:${addrs wlan0}  \
up:${upspeed wlan0}  \
down:${downspeed wlan0}
